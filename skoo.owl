<rdfs:label xml:lang="en">Formula</rdfs:label>
<ns:term_status xml:lang="en">stable</ns:term_status>
</owl:Class>
<!--  http://purl.org/net/skoo#Geographical_Object  -->
<owl:Class rdf:about="http://purl.org/net/skoo#Geographical_Object">
