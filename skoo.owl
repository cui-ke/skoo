</owl:Class>
<!--  http://purl.org/net/skoo#Grammar  -->
<owl:Class rdf:about="http://purl.org/net/skoo#Grammar">
<rdfs:subClassOf rdf:resource="http://purl.org/net/skoo#LanguageDefinitionObject"/>
<rdfs:subClassOf>
<owl:Restriction>
<owl:onProperty rdf:resource="http://www.w3.org/2001/sw/BestPractices/OEP/SimplePartWhole/part.owl#hasPart"/>
<owl:someValuesFrom rdf:resource="http://purl.org/net/skoo#Phrase"/>
</owl:Restriction>
</rdfs:subClassOf>
