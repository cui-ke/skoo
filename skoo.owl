</owl:Restriction>
</rdfs:subClassOf>
<rdfs:label xml:lang="en">Linguistic object</rdfs:label>
<ns:term_status xml:lang="en">stable</ns:term_status>
</owl:Class>
<!--  http://purl.org/net/skoo#Measuring  -->
<owl:Class rdf:about="http://purl.org/net/skoo#Measuring">
